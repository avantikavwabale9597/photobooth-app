{
  "name": "photobooth-app",
  "version": "1.0.0",
  "description": "A desktop photobooth app built using Electron.js",
  "main": "main.js",
  "type": "commonjs",
  "author": "Avantika Wabale",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.ava.photobooth",
    "productName": "PhotoBooth by Ava",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
